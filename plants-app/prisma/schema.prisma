// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model rasperry_pi {
  id      Int      @id @default(autoincrement())
  piName  String
  ip      String
  // plants  Plant[]
}

model Plant {
  id       Int          @id @default(autoincrement())
  time     DateTime     @default(now())
  temperature   Float
  light         Float
  humidity      Float
  gas_conc      Float
}