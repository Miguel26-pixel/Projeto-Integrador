// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RasperryPI {
  id      Int      @id @default(autoincrement())
  piName  String
  ip      String
  plants  Plant[]
}

model Plant {
  id            Int          @id @default(autoincrement())
  time          DateTime     @default(now())
  temperature   Float
  light         Float
  humidity      Float
  gasConc       Float
  pi            RasperryPI  @relation(fields: [piID], references: [id])
  piID          Int

  @@unique([id])
}